<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="interfaz.Principal">
  <grid id="rootPanel" binding="rootPanel" layout-manager="BorderLayout" hgap="10" vgap="10">
    <constraints>
      <xy x="0" y="0" width="1100" height="720"/>
    </constraints>
    <properties>
      <background color="-1511169"/>
    </properties>
    <border type="empty">
      <size top="10" left="10" bottom="10" right="10"/>
    </border>
    <children>
      <grid id="panelTop" binding="panelTop" layout-manager="BorderLayout" hgap="0" vgap="8">
        <constraints border-constraint="North"/>
        <properties>
          <background color="-1511169"/>
        </properties>
        <border type="empty">
          <size top="12" left="12" bottom="12" right="12"/>
        </border>
        <children>
          <grid id="panelFields" binding="panelFields" layout-manager="GridLayoutManager" row-count="2" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="0" vgap="0">
            <margin top="5" left="5" bottom="5" right="5"/>
            <constraints border-constraint="North"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="lblBase" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false">
                    <preferred-size width="153" height="18"/>
                  </grid>
                </constraints>
                <properties>
                  <text value="Base IVA (.csv o .xlsx)"/>
                </properties>
              </component>
              <component id="txtBase" class="javax.swing.JTextField" binding="txtBase">
                <constraints>
                  <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="2" anchor="8" fill="2" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <columns value="50"/>
                </properties>
              </component>
              <component id="btnBuscarBase" class="javax.swing.JButton" binding="btnBuscarBase">
                <constraints>
                  <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Buscar"/>
                </properties>
              </component>
              <component id="lblReporte" class="javax.swing.JLabel">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false">
                    <preferred-size width="153" height="18"/>
                  </grid>
                </constraints>
                <properties>
                  <text value="Reporte Amazon (.txt)"/>
                </properties>
              </component>
              <component id="txtReporte" class="javax.swing.JTextField" binding="txtReporte">
                <constraints>
                  <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="2" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <columns value="50"/>
                </properties>
              </component>
              <component id="btnBuscarReporte" class="javax.swing.JButton" binding="btnBuscarReporte">
                <constraints>
                  <grid row="1" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Buscar"/>
                </properties>
              </component>
            </children>
          </grid>
          <grid id="panelButtons" binding="panelButtons" layout-manager="FlowLayout" hgap="10" vgap="0" flow-align="0">
            <constraints border-constraint="South"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="btnPreview" class="javax.swing.JButton" binding="btnPreview">
                <constraints/>
                <properties>
                  <text value="Procesar"/>
                </properties>
              </component>
              <component id="btnClear" class="javax.swing.JButton" binding="btnClear">
                <constraints/>
                <properties>
                  <text value="Limpiar"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <component id="scrollPane" class="javax.swing.JScrollPane" binding="scrollPane">
        <constraints border-constraint="Center"/>
        <properties>
          <background color="-1511169"/>
          <enabled value="true"/>
        </properties>
      </component>
      <grid id="panelStatus" binding="panelStatus" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="empty">
          <size top="5" left="10" bottom="10" right="10"/>
        </border>
        <children>
          <component id="lblStatus" class="javax.swing.JLabel" binding="lblStatus">
            <constraints border-constraint="Center"/>
            <properties>
              <text value="Listo. Arrastra archivos .csv/.xlsx y .txt o usa Buscar."/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
